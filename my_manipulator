#include <stdexcept>
#include <string>
#include <iomanip>
using namespace std;


ostream &testout (ostream &output)
{
        cout <<"good name ";
        return output;
}

istream &testin (istream &input)
{
    cout <<"enter the name ";
    return input;
}


int main()
{
