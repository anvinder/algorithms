#include <iostream>
#include<iomanip>
#include<stdio.h>
#include<stdlib.h>

using namespace std;

ostream &choice(ostream &output)
{
    output<<"-->";
    return output;
}

istream &getname(istream &input)
{
    cout<<"name please: ";
    return input;
}

int main ()
{
//cout<<setw(10)<<setfill('*')<<10<<endl;
string name;
cin>>getname>>name;
cout << choice << name;
    return 0;
}
