#include<iostream>
#include<stdlib.h>
#include<stdio.h>

using namespace std;

class father
{
public:
void introduce()
{
    cout<<"father class called"<<endl;
}
};

class child: public father
{
public:
    void introduce()
    {
        cout<<" child class called";
    }
};


int main ()
{

child c;
//c.introduce();
c.father::introduce();

    return 0;
}
