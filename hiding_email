#include <stdio.h>
#include <string.h>
#include <cstring>
#include <iostream>
#include <sstream>
#include<string>

using namespace std;

int main ()
{
  string str1 ("abdhskfnef.jgbujbsd@gmail.com");
  string str2 ("@");
string::size_type found=str1.find(str2);
//cout <<found;
  for (int i=1; i<found-1; i++)
    str1[i]='*';
  cout <<str1;

  return 0;
}
